 */
public class Task23Line {
	private Point p1;
	private Point p2;

	/**
	 * Getter method for p1.
	 * 
	 * @return the p1
	 */
	public Point getP1() {
		return p1;
	}

	/**
	 * Setter method for p1.
	 * 
	 * @param p1
	 *            the p1 to set
	 */
	public void setP1(Point p1) {
		this.p1 = p1;
	}

	/**
	 * Getter method for p2.
	 * 
	 * @return the p2
	 */
